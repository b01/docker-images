#This script removes and build all images. 

# Remove all associated containers.
docker rm centos-apps centos-apps54 centos-nginx centos-mongodb centos-memcached centos-php centos-php54 centos-mysql

# Remove all images.
docker rmi centos-apps centos-apps54 centos-nginx centos-mongodb centos-memcached centos-php centos-php:54 centos-mysql
